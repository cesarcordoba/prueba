    .arquitecto-container(*ngIf="arquitecto")
        mat-toolbar
            mat-toolbar-row(style="justify-content : space-between")
                .titulo
                    p Información del Arquitecto
        .arquitecto-content
            .volumen
                p Cédula Profesional:
                h4 {{arquitecto.cedula}}
    .constructora-container(*ngIf="constructora")
        mat-toolbar
            mat-toolbar-row(style="justify-content : space-between")
                .titulo
                    p Información de la Constructora
        .constructora-content
            .nombreConstructora
                p Nombre:
                h4 {{constructora.nombreConstructora}}
            .director
                p Director:
                h4 {{arquitecto.director}}
            .direccion
                p Direccion:
                h4 {{arquitecto.direccion}}
            .fecha_constitucion
                p Fecha de constitución:
                h4 {{arquitecto.fecha_constitucion}}
            .rfc
                p Cédula Profesional:
                h4 {{arquitecto.rfc}}
