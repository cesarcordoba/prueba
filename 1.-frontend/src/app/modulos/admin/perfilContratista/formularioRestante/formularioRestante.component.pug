mat-card.formulario-contratista-complemento
    mat-toolbar
        mat-toolbar-row(style="justify-content : space-between")
            .titulo
                h3 Información Complementaria
    .info-preview
        .direccion
            p Dirección: 
            h4 {{ contratista?.direccion }}
        .telefono
            p Teléfono: 
            h4 {{ contratista?.telefono }}
        .paginaWeb
            p Pagina Web: 
            h4 {{ contratista?.paginaWeb }}
        .numPromAnaualCasas
            p Número promedio anual de casas:
            h4 {{ contratista?.numPromAnaualCasas }}
        .costoPromedio
            p Costo promedio: 
            h4 {{ contratista?.costoPromedio }}
        .volumen
            p Volumen: 
            h4 {{ contratista?.volumen }}
