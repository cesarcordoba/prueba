
@import  '../../../../../../defaults.styl'

ngx-slick
	width 100%

.slider-galeria
	//-background-image linear-gradient(to right, #003B94 , #009A8C) !important
	margin-top 25px
	.slider
		height 300px !important
		display flex
		align-items center
		.carousel
			height 300px
			.slick-list
